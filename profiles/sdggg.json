{
  "name": "sdggg",
  "description": "ertre",
  "case_type": "General Analysis",
  "ingestion": {
    "system_context": "You are an expert analyst extracting entities and relationships from documents. Focus on identifying key people, organizations, locations, events, dates, and their connections. Capture relevant details that establish relationships between entities. Be thorough but precise in your extractions.",
    "special_entity_types": [],
    "temperature": 1.0
  },
  "chat": {
    "system_context": "You are an AI assistant helping to analyze and understand documents.",
    "analysis_guidance": "Provide clear explanations and highlight important connections.",
    "temperature": 1.0
  },
  "folder_processing": {
    "type": "special",
    "file_rules": [
      {
        "pattern": "*.wav,*.WAV",
        "role": "audio",
        "actions": [
          "transcribe",
          "translate"
        ],
        "transcribe_languages": [
          "en"
        ],
        "translate_languages": [
          "es",
          "fr"
        ],
        "whisper_model": "base"
      },
      {
        "pattern": "*.sri,*.sri.XML,*.SRI,*.SRI.XML",
        "role": "metadata",
        "parser": "sri",
        "metadata_extraction": {
          "fields": {
            "call_id": [
              "//Call/@id",
              "//call/@id",
              "//metadata/callId/text()"
            ],
            "start_time": [
              "//Call/@startTime",
              "//call/@startTime",
              "//startTime/text()"
            ],
            "end_time": [
              "//Call/@endTime",
              "//call/@endTime",
              "//endTime/text()"
            ],
            "duration": [
              "//Call/@duration",
              "//call/@duration",
              "//duration/text()"
            ],
            "direction": [
              "//Call/@direction",
              "//direction/text()"
            ],
            "caller_number": [
              "//Parties/Caller/@number",
              "//caller/number/text()"
            ],
            "callee_number": [
              "//Parties/Callee/@number",
              "//callee/number/text()"
            ],
            "agent_id": [
              "//Agent/@id",
              "//agent/id/text()"
            ],
            "channel_count": [
              "//Audio/@channels",
              "//audio/channels/text()"
            ],
            "sample_rate": [
              "//Audio/@sampleRate",
              "//audio/sampleRate/text()"
            ]
          },
          "time_alignment": true,
          "merge_strategy": "prefer_xml_over_plaintext"
        }
      },
      {
        "pattern": "*.wav.XML,*.WAV.XML",
        "role": "metadata",
        "parser": "wav_xml",
        "metadata_extraction": {
          "fields": {
            "sample_rate": [
              "//audio/sampleRate/text()",
              "//WAV/@sampleRate"
            ],
            "bit_depth": [
              "//audio/bitDepth/text()",
              "//WAV/@bitDepth"
            ],
            "channels": [
              "//audio/channels/text()",
              "//WAV/@channels"
            ],
            "file_size": [
              "//audio/fileSize/text()"
            ],
            "md5": [
              "//checksums/md5/text()",
              "//hashes/md5/text()"
            ]
          },
          "merge_strategy": "augment"
        }
      },
      {
        "pattern": "*_sri.xdb,*_sri.xdb.XML,*_sri.xsc,*_sri.xsc.XML",
        "role": "metadata",
        "parser": "index_bundle",
        "metadata_extraction": {
          "fields": {
            "index_terms": [
              "//Index//Term/text()",
              "//index/term/text()"
            ],
            "engine": [
              "//Index/@engine",
              "//index/@engine"
            ],
            "version": [
              "//Index/@version",
              "//index/@version"
            ]
          },
          "merge_strategy": "augment"
        }
      },
      {
        "pattern": "*_syn.rtf,*_syn.rtf.XML",
        "role": "interpretation",
        "parser": "rtf",
        "extract_participants": true,
        "extract_interpretation": true
      }
    ],
    "processing_rules": "Treat files that share the same base name as a single related bundle. Use the .WAV file as the primary audio source. Parse .sri (and its XML) as call/session metadata and timing marks to align transcript segments. Ingest any .wav.XML as audio technical metadata. Include *_sri.xdb/.xsc (and XML) as auxiliary index/schema metadata if present. Ingest *_syn.rtf (and XML) as human interpretation/synopsis; extract participants and interpretation notes and link them to the call. Merge all extracted metadata, alignments, and interpretations into the transcript. If multiple sources provide the same field, prefer explicit XML values over plaintext; otherwise augment without overwriting. Produce one consolidated record per base name.",
    "output_format": "wiretap_structured",
    "related_files_indicator": true
  }
}